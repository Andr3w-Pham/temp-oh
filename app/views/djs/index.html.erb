<p id="notice"><%= notice %></p>

<h1>Djs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Location</th>
      <th>Portfolio</th>
      <th>Rate</th>
      <th>Image</th>
      <th>Dj check</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @djs.each do |dj| %>
      <tr>
        <td><%= dj.name %></td>
        <td><%= dj.description %></td>
        <td><%= dj.location %></td>
        <td><%= dj.portfolio %></td>
        <td><%= dj.rate %></td>
        <td><%= dj.image %></td>
        <td><%= dj.dj_check %></td>
        <td><%= dj.user %></td>
        <td><%= link_to 'Show', dj %></td>
        <td><%= link_to 'Edit', edit_dj_path(dj) %></td>
        <td><%= link_to 'Destroy', dj, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dj', new_dj_path %>
